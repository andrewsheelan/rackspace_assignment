<%= simple_form_for @veterinary_doctor do |f| %>  
  <%= f.input :name %>   
  <%= f.input :address_line1 %>   
  <%= f.input :address_line2 %>   
  <%= f.input :city %>   
  <%= f.input :state %>   
  <%= f.input :zip %>   
  <%= f.input :years_in_practice %>   
  <%= f.button :submit, class: 'btn'  %>  
<% end %>  